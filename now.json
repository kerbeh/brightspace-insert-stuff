{
    "version": 2,
    "builds": [
        {
            "src": "dist/*",
            "use": "@now/static"
        },
        {
            "src": "app.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/dist/index.html"
        },
        {
            "src": "/favicon.ico",
            "dest": "/dist/favicon.ico"
        },
        {
            "src": "/search",
            "dest": "/app.js"
        }
    ]
}